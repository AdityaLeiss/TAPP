<p id="notice"><%= notice %></p>

<h1>Dosens</h1>

<table>
  <thead>
    <tr>
      <th>Nip</th>
      <th>Nama</th>
      <th>Email</th>
      <th>Username</th>
      <th>Password</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dosens.each do |dosen| %>
      <tr>
        <td><%= dosen.nip %></td>
        <td><%= dosen.nama %></td>
        <td><%= dosen.email %></td>
        <td><%= dosen.username %></td>
        <td><%= dosen.password %></td>
        <td><%= link_to 'Show', dosen %></td>
        <td><%= link_to 'Edit', edit_dosen_path(dosen) %></td>
        <td><%= link_to 'Destroy', dosen, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dosen', new_dosen_path %>
<%= link_to "Logout", destroy_user_session_path, method: :delete, :class => 'navbar-link'  %>
