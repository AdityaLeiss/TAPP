
<h1>Babs</h1>

<table>
  <thead>
    <tr>
      <th>Nim</th>
      <th>Nama</th>
      <th>Bab</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hdosens.each do |hdosen| %>
      <tr>
        <td><%= hdosen.nim %></td>
        <td><%= hdosen.nama %></td>
        <td><%= hdosen.bab %></td>
        <td><%= hdosen.progress %></td>
        <td><%= link_to 'Show', hdosen_path(hdosen) %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>



<%= link_to "Logout", destroy_mdosen_session_path, method: :delete, :class => 'navbar-link'  %>
<%= link_to "Change your password", edit_mdosen_registration_path %>
<%= link_to 'Back', bimdosens_path %>
